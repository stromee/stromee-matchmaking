{
	"compilerOptions": {
		"strict": true,
		"outDir": "bin",
		"target": "es2019",
		"module": "CommonJS",
		"moduleResolution": "node",
		"sourceMap": true,
		"experimentalDecorators": true,
		"pretty": true,
		"noFallthroughCasesInSwitch": true,
